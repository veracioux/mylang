error MyError "my error"

# throw with no args
try (
    throw
) catch e (
    Error (
        # . at the end avoids trailing space
        echo "1 Caught error of type Error with message:" $e "."
    )
)

# throw with a string arg
try (
    throw Error "err msg"
) catch e (
    Error (
        echo "2 Caught error of type Error with message" $e
    )
)

# throw with concrete type
try (
    throw MyError
) catch e (
    MyError (
        echo "3 Caught error of type MyError with message" $e
    )
    Error (
        echo "4 Caught error of type Error with message" $e
    )
)
